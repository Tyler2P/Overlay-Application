<Page x:Class="BillybobbeepOverlay.Frames.Overlay"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BillybobbeepOverlay.Frames"
      mc:Ignorable="d" 
      d:DesignHeight="220" d:DesignWidth="390"
      Title="Overlay"
      Background="Transparent">
    <Grid>
        <TextBlock x:Name="Title" HorizontalAlignment="Left" Margin="125,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="50" Width="248" FontSize="25" Foreground="White" FontWeight="Bold" Text="Example Title"/>
        <TextBlock x:Name="Description" HorizontalAlignment="Left" Margin="125,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="73" Width="248" FontSize="15" Foreground="White" Text="Example Description"/>
        <Image x:Name="Logo" HorizontalAlignment="Left" Height="100" Margin="20,59,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Page>
